<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorSetPage" prefHeight="730.0" prefWidth="950.0" style="-fx-background-color: #5D6D7E; -fx-background-radius: 50px;" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.SetPageController">
   <children>
      <JFXComboBox fx:id="comboSetName" layoutX="313.0" layoutY="46.0" onAction="#clickComboSetName" prefHeight="25.0" prefWidth="303.0" />
      <Label layoutX="190.0" layoutY="52.0" text="Set Name:" textFill="WHITE">
         <font>
            <Font name="Arial Black" size="18.0" />
         </font></Label>
      <JFXButton fx:id="buttonNewSet" layoutX="634.0" layoutY="46.0" onAction="#clickButtonNewSet" style="-fx-background-color: #F9E79F;" text="New Set">
         <font>
            <Font name="Arial Black" size="18.0" />
         </font></JFXButton>
      <TableView fx:id="table" layoutX="78.0" layoutY="152.0" prefHeight="426.0" prefWidth="428.0" style="-fx-background-color: #ECF0F1; -fx-background-radius: 5 5 5 5;">
        <columns>
          <TableColumn fx:id="firstWordColumn" prefWidth="215.0" reorderable="false" style="-fx-alignment: Center; -fx-background-radius: 5 5 5 5;" text="First Row" />
          <TableColumn fx:id="secondWordColumn" minWidth="0.0" prefWidth="211.0" reorderable="false" style="-fx-alignment: Center; -fx-background-radius: 5 5 5 5;" text="Second Row" />
        </columns>
      </TableView>
      <JFXButton fx:id="buttonStudy" layoutX="573.0" layoutY="252.0" onAction="#clickButtonStudy" prefHeight="87.0" prefWidth="320.0" style="-fx-background-color: #7DCEA0;" text="Study">
         <font>
            <Font name="Arial Black" size="18.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="buttonArrangeSet" layoutX="573.0" layoutY="392.0" onAction="#clickButtonArrangeSet" prefHeight="87.0" prefWidth="320.0" style="-fx-background-color: #7DCEA0;" text="Arrange Set">
         <font>
            <Font name="Arial Black" size="18.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
